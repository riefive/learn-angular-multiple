{
  "name": "learn-angular-multiple",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project src",
    "start:one": "ng serve --project applicationOne",
    "start:two": "ng serve --project applicationTwo",
    "build": "ng build --prod --project src",
    "build:one": "ng build --prod --project applicationOne",
    "build:two": "ng build --prod --project applicationTwo",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run src:serve-ssr",
    "serve:ssr": "node dist/src/server/main.js",
    "build:ssr": "ng build && ng run src:server",
    "prerender": "ng run src:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@ngrx/store": "^12.5.1",
    "@nguniversal/express-engine": "^12.1.3",
    "express": "^4.15.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.0",
    "@nguniversal/builders": "^12.1.3",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}